{
  "name": "ecoswap",
  "version": "1.0.0",
  "description": "A platform for swapping unused or lightly-used items to promote sustainability",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend; npm run dev",
    "client": "cd frontend; npm start",
    "install-all": "npm install; cd backend; npm install; cd ../frontend; npm install",
    "build": "cd frontend; npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "concurrently \"cd backend; npm test\" \"cd frontend; npm test\"",
    "lint": "concurrently \"cd backend; npm run lint\" \"cd frontend; npm run lint\"",
    "seed": "cd backend; npm run seed",
    "server-win": "cd backend && npm run dev",
    "client-win": "cd frontend && npm start",
    "dev-win": "concurrently \"npm run server-win\" \"npm run client-win\""
  },
  "keywords": [
    "sustainability",
    "swap",
    "reuse",
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/ecoswap.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ecoswap/issues"
  },
  "homepage": "https://github.com/yourusername/ecoswap#readme",
  "dependencies": {
    "mongodb": "^6.15.0"
  }
}
